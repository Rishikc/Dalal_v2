<div id="notification_modal_partial">
  <div class="row">
   <div class="col-lg-12">
     <div class="panel panel-default">
       <div class="panel-heading">
         <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Notifications</h3>
       </div>
	   	<div class="panel-body"	>
        <%@notifications_paginate.each do |notice|%>
        <a href="#" class="list-group-item">
        <span class="badge"><%= notice.updated_at%></span>
        <i class="fa fa-fw fa-check"></i> <%= notice.notification%> </a> 
        <% end %> 
        <%= paginate @notifications_paginate, :remote => true, :window => 2, :outer_window => 3%>
        <%= page_entries_info @notifications_paginate %>
   	    </div>
     </div>
   </div>
  </div>
</div>