<div id="notification_modal_partial">
  <div class="row">
   <div class="col-lg-12">
     <div class="panel panel-default">
       <div class="panel-heading">
         <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Notifications</h3>
       </div>
	   	<div class="panel-body"	>
        <%@notifications_paginate.each do |notice|%>
        <a href="#" class="list-group-item">
        <span class="badge"><%= notice.updated_at.strftime("%d%h%r")%></span>
        <i class="fa fa-fw fa-check"></i> <%= notice.notification%> </a> 
        <% end %>

         <div id="paginator">
         <%= mojo_paginator(@notifications_count,'notice',@active) %>
        </div> 

   	    </div>
     </div>
   </div>
  </div>
</div>