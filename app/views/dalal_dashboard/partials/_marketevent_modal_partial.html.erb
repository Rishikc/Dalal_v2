<div id="marketevent_modal_partial"> 
  <div class="row">
   <div class="col-lg-12">

     <div class="panel panel-default">
       <div class="panel-heading">
         <h3 class="panel-title"><i class="fa fa-money fa-fw"></i> Market Event</h3>
       </div>
	   	  <div class="panel-body">
        
        <%@market_events_paginate.each do |event|%>
        <a class="list-group-item">
        <span class="badge"><%= event.updated_at%></span>
        <i class="fa fa-fw fa-check"></i> <%= event.eventname%> </a> 
        <% end %>

        <div id="paginator">
         <% @market_events_count.to_i.times do |i| %>
         <a href="#paginator" <%= "onclick=update_modal_partials('#{i}','market');" %> ><%="#{i}"%></a>
         <% end %>  
        </div>

   	    </div>
     </div>
     
   </div>
  </div>
</div>